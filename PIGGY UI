    -- Auto Farm Function 
    local autofarm = function ()
            for i , v in pairs(game:GetService("Workspace").Folders.Monsters:GetChildren()) do
                if v:FindFirstChild("HumanoidRootPart")  then
                        local Name = v.Name
                    for i2 , v2 in pairs(game:GetService("Workspace").Folders.Monsters:GetChildren()) do
                        if v2.Name == tostring(Name) then
                            if v2:FindFirstChild("EnemyHealthBarGui") then
                                if v2.EnemyHealthBarGui.HealthText.Text ~= 0 then
                                    repeat  wait()             
                                        for a , b in pairs(game:GetService("Workspace").Folders.Monsters:GetChildren()) do
                                            if string.find(b.Name, "Titan") then
                                                toTarget(game:GetService("Players").LocalPlayer.Character.HumanoidRootPart.Position,v.HumanoidRootPart.Position,v.HumanoidRootPart.CFrame *CFrame.new(0,9.4,0) * CFrame.Angles(math.rad(-100), 0, 0) )                                      
                                                game.Workspace.CurrentCamera.CameraSubject = v.Monster
                                            end
                                        end
                                        for a2 , b2 in pairs(game:GetService("Workspace").Folders.Monsters:GetChildren()) do
                                            if string.find(b2.Name, "Demon") then
                                                toTarget(game:GetService("Players").LocalPlayer.Character.HumanoidRootPart.Position,v.HumanoidRootPart.Position,v.HumanoidRootPart.CFrame *CFrame.new(0,-8,0) * CFrame.Angles(math.rad(90), 0, 0) )                                      
                                                game.Workspace.CurrentCamera.CameraSubject = v.Monster
                                            end
                                            if string.find(b2.Name, "Emmu") then
                                                toTarget(game:GetService("Players").LocalPlayer.Character.HumanoidRootPart.Position,v.HumanoidRootPart.Position,v.HumanoidRootPart.CFrame *CFrame.new(0,-8,0) * CFrame.Angles(math.rad(90), 0, 0) )                                      
                                                game.Workspace.CurrentCamera.CameraSubject = v.Monster
                                            end
                                            if string.find(b2.Name, "Yakaza") then
                                                toTarget(game:GetService("Players").LocalPlayer.Character.HumanoidRootPart.Position,v.HumanoidRootPart.Position,v.HumanoidRootPart.CFrame *CFrame.new(0,-8,0) * CFrame.Angles(math.rad(90), 0, 0) )                                      
                                                game.Workspace.CurrentCamera.CameraSubject = v.Monster
                                            end
                                        end
                                        for i , v in pairs(game:GetService("Workspace").Folders.Monsters:GetChildren()) do
                                            if string.find(v.Name,"Virtual")  then
                                               local LOL = v.Name 
                                               for i2 , v2 in pairs(game:GetService("Workspace").Folders.SpawnPoints:GetChildren()) do
                                                   if v2.Name == "SpawnLocation1" and game:GetService("Workspace").Folders.Monsters:FindFirstChild(tostring(LOL)) then
                                                         toTarget(game:GetService("Players").LocalPlayer.Character.HumanoidRootPart.Position,v2.Position,v2.CFrame *CFrame.new(0,9.1,0) * CFrame.Angles(math.rad(-90), 0, 0) )                                     
                                                      end
                                                end
                                            end
                                        end
              --[[
                                        if game.PlaceId == 6990129309 then
                                        for i22 , v22 in pairs(game:GetService("Workspace").Folders.Monsters:GetChildren()) do 
                                            if string.find(v22.Name,"Virtual") then
                                                local Lol = v22.Name 
                                            end
    --Virtual                                 
                                        end
                                        for i11 ,  v11 in pairs(game:GetService("Workspace").Folders.SpawnPoints:GetChildren()) do
                                            if v11.Name == "SpawnLocation1" and   then
                                                toTarget(game:GetService("Players").LocalPlayer.Character.HumanoidRootPart.Position,v11.Position,v11.CFrame *CFrame.new(0,9.1,0) * CFrame.Angles(math.rad(-90), 0, 0) )                                      
                                                game:GetService('RunService').Stepped:connect(function()
                                                    if set.farm  then
                                                    game.Players.LocalPlayer.Character.Humanoid:ChangeState(11)
                                                    end
                                                 end)
                                            end
                                        end
                                    end
              ]]
                                      
                                           if (v.HumanoidRootPart.Position - plr.Character.HumanoidRootPart.Position).magnitude < 40 then
                                            for i = 3 , 5 , 1 do 
                                                if i == 5 then return i == 1 end 
                                                local args = {
                                                    [1] = "UseSkill",
                                                    [2] = {
                                                        ["hrpCFrame"] = v.HumanoidRootPart.CFrame,
                                                        ["attackNumber"]  = i 
                                                    },
                                                    [3] = "BasicAttack"
                                                }
                                                
                                                game:GetService("ReplicatedStorage").RemoteEvents.MainRemoteEvent:FireServer(unpack(args))
                                                end                                    
                                            end          
                                   
                                            game:GetService('RunService').Stepped:connect(function()
                                                if set.farm  then
                                                game.Players.LocalPlayer.Character.Humanoid:ChangeState(11)
                                                end
                                            end)
                                    until v.EnemyHealthBarGui.HealthText.Text == 0  or not  set.farm
                                 
                            end
                        end
                    end
                end
            end
        end
    end
    
    
    
    
    -- Start 
    local Start = function ()
        local args = {
            [1] = "CreateRoom",
            [2] = {
                ["Difficulty"] = tostring(set.secmode),
                ["FriendsOnly"] = set.Friends,
                ["MapName"] = tostring(set.secmap),
                ["Hardcore"] =  set.Hardcore
            }
        }
        game:GetService("ReplicatedStorage").RemoteFunctions.MainRemoteFunction:InvokeServer(unpack(args))
        game:GetService("ReplicatedStorage").RemoteFunctions.MainRemoteFunction:InvokeServer("TeleportPlayers")
    end
    
    
